          "format": "uri"
        },
        "playerLevel": {
          "type": "integer"
        },
        "PlayerTrophies": {
          "type": "integer"
        },
        "TownHallLevel": {
          "type": "integer"
        },
        "BuilderHallLevel": {
          "type": "integer"
        },
        "AtttackWins": {
          "type": "integer"
        },
        "DefenseWins": {
          "type": "integer"
        },
        "LeagueID": {
          "type": "integer"
        },
        "AchievementsCount": {
          "type": "integer"
        },
        "TroopsCount": {
          "type": "integer"
        },
        "HeroesCount": {
          "type": "integer"
        },
        "SpellsCount": {
          "type": "integer"
        },
        "Donations": {
          "type": "integer"
        },
        "DonationsReceived": {
          "type": "integer"
        },
        "HasClan": {
          "type": "integer"
        },
        "ClanLevel": {
          "type": "integer"
        }
      }
    }
