        },
        "type": {
          "type": "string"
        }
      }
    },
    "subjectPosition": {
      "type": "string",
      "enum": [
        "none",
        "index",
        "value"
      ]
    },
    "merklizationRootPosition": {
      "type": "string",
      "enum": [
        "none",
        "index",
        "value"
      ]
    },
    "revNonce": {
      "type": "integer"
    },
    "version": {
      "type": "integer"
    },
    "updatable": {
      "type": "boolean"
    },
    "credentialSubject": {
      "type": "object",
      "required": [
        "id",
        "PlayerLevel",
        "PlayerTrophies",
        "TownHallLevel",
        "BuilderHallLevel",
        "AtttackWins",
        "DefenseWins",
        "LeagueID",
        "AchievementsCount",
        "TroopsCount",
        "HeroesCount",
        "SpellsCount",
        "Donations",
